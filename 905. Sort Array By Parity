class Solution(object):
    def sortArrayByParity(self, nums):
        i=0
        j=0
        final1=[]
        final2=[]
        for i in nums:
            if i%2==0:
                final1.append(i)
        for j in nums:
            if j%2!=0:
                final1.append(j)
        result=final1+final2
        return result
        
